<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
   <application>
        <locale-config>
            <default-locale>es</default-locale>
        </locale-config>
        <resource-bundle>
            <base-name>language</base-name>
            <var>msg</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/abm/usuarios/ConsultarUsuarios.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETALLE_USUARIO</from-outcome>
            <to-view-id>/abm/usuarios/EditarUsuario.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/usuarios/EditarUsuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LISTA_USUARIO</from-outcome>
            <to-view-id>/abm/usuarios/ConsultarUsuarios.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/autos/EditarAuto.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LISTA_AUTO</from-outcome>
            <to-view-id>/abm/autos/ConsultarAutos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/autos/ConsultarAutos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETALLE_AUTO</from-outcome>
            <to-view-id>/abm/autos/EditarAuto.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/clientes/ConsultarClientes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETALLE_CLIENTE</from-outcome>
            <to-view-id>/abm/clientes/EditarCliente.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/clientes/EditarCliente.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LISTA_CLIENTE</from-outcome>
            <to-view-id>/abm/clientes/ConsultarClientes.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mov/alquiler/EditarAlquiler.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>ConsultarAlquiler</from-outcome>
            <to-view-id>/mov/alquiler/ConsultarAlquiler.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mov/alquiler/ConsultarAlquiler.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETALLE_ALQUILERES</from-outcome>
            <to-view-id>/mov/alquiler/EditarAlquiler.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <application>
        <action-listener>org.primefaces.application.DialogActionListener</action-listener>
        <navigation-handler>org.primefaces.application.DialogNavigationHandler</navigation-handler>
        <view-handler>org.primefaces.application.DialogViewHandler</view-handler>
    </application>
    <navigation-rule>
        <from-view-id>/abm/seguro/EditarSeguro.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LISTA_SEGURO</from-outcome>
            <to-view-id>/abm/seguro/ConsultarSeguros.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/seguro/ConsultarSeguros.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETALLE_SEGURO</from-outcome>
            <to-view-id>/abm/seguro/EditarSeguro.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/tipomovimiento/ConsultarTMovimientos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DETALLE_TMOVIMIENTO</from-outcome>
            <to-view-id>/abm/tipomovimiento/EditarTMovimiento.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/abm/tipomovimiento/EditarTMovimiento.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LISTA_TMOVIMIENTO</from-outcome>
            <to-view-id>/abm/tipomovimiento/ConsultarTMovimientos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/usuario/InformeUsuario.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_USUARIO</from-outcome>
            <to-view-id>/inf/usuario/InformeUsuarioPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/autos/InformeAutos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_AUTOS</from-outcome>
            <to-view-id>/inf/autos/InformeAutosPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/clientes/InformeClientes.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_CLIENTE</from-outcome>
            <to-view-id>/inf/clientes/InformeClientesPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/tipomovimientos/InformeTMovimientos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_TMOVIMIENTO</from-outcome>
            <to-view-id>/inf/tipomovimientos/InformeTMovimientosPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/alquileres/InformeAlquileres.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_ALQUILER</from-outcome>
            <to-view-id>/inf/alquileres/InformeAlquileresPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/seguros/InformeSeguros.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_SEGURO</from-outcome>
            <to-view-id>/inf/seguros/InformeSegurosPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/cuotasautos/InformeCuotasAutos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_CUOTAS</from-outcome>
            <to-view-id>/inf/cuotasautos/InformeCuotasAutosPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/inf/movimientos/InformeMovimientos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>INFORME_MOVIMIENTO</from-outcome>
            <to-view-id>/inf/movimientos/InformeMovimientosPdf.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/login/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LOGIN_OK</from-outcome>
            <to-view-id>/templates/template.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>LOGIN_NOK</from-outcome>
            <to-view-id>/faces/login/login.xhtml</to-view-id>
        </navigation-case>

    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/abm/autos/ConsultarAutos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LISTA_CUOTA</from-outcome>
            <to-view-id>/mov/cuotasAutos/CuotasAutos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/mov/cuotasAutos/CuotasAutos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>LISTA_AUTO</from-outcome>
            <to-view-id>/abm/autos/ConsultarAutos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>

</faces-config>


